<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Count_must_be_non_negative</fullName>
    <active>true</active>
    <description>The count cannot be negative</description>
    <errorConditionFormula>Count__c &lt; 0</errorConditionFormula>
    <errorDisplayField>Count__c</errorDisplayField>
    <errorMessage>You must enter a positive number</errorMessage>
</ValidationRule>
